<table class="table">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
    </tr>
  </thead>
  <tbody>
    <% doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.email %></td>
        <td><%= doctor.first_name %></td>
        <td><%= doctor.last_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
