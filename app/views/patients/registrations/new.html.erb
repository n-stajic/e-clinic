<h2>Patient Sign Up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render partial: "common_fields", locals: {f: f, dissabled_fields: false} %>

  <br/>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<br/>

<%= render "patients/shared/links" %>
